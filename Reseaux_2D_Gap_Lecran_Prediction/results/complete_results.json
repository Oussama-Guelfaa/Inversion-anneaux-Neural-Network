  "config": {
    "experiment": {
      "name": "Dual_Gap_Lecran_Prediction",
      "description": "Joint prediction of gap and L_ecran parameters from holographic intensity profiles",
      "version": "1.0"
    },
    "model": {
      "name": "DualParameterPredictor",
      "type": "Dense_Network_Dual_Output",
      "input_size": 600,
      "output_size": 2
    },
    "architecture": {
      "dense_layers": [
        {
          "size": 512,
          "activation": "relu",
          "batch_norm": true,
          "dropout": 0.2
        },
        {
          "size": 256,
          "activation": "relu",
          "batch_norm": true,
          "dropout": 0.2
        },
        {
          "size": 128,
          "activation": "relu",
          "batch_norm": true,
          "dropout": 0.2
        },
        {
          "size": 64,
          "activation": "relu",
          "batch_norm": true,
          "dropout": 0.1
        },
        {
          "size": 2,
          "activation": "linear"
        }
      ]
    },
    "training": {
      "batch_size": 32,
      "epochs": 200,
      "learning_rate": 0.001,
      "optimizer": "Adam",
      "weight_decay": "1e-4",
      "loss_function": "MSE",
      "early_stopping": {
        "patience": 30,
        "min_delta": 0.0001,
        "monitor": "val_loss",
        "restore_best_weights": true
      },
      "scheduler": {
        "type": "ReduceLROnPlateau",
        "mode": "min",
        "factor": 0.5,
        "patience": 15,
        "min_lr": "1e-6"
      }
    },
    "data_processing": {
      "augmentation": {
        "enable": true,
        "gap_density": 2,
        "L_ecran_density": 2,
        "method": "linear",
        "include_original": true
      },
      "data_splits": {
        "train": 0.64,
        "validation": 0.16,
        "test": 0.2
      },
      "normalization": {
        "input_scaler": "StandardScaler",
        "target_scaling": {
          "gap_scaler": "StandardScaler",
          "L_ecran_scaler": "StandardScaler",
          "separate_scaling": true
        }
      },
      "profile_truncation": 600
    },
    "evaluation": {
      "performance_targets": {
        "gap_accuracy": 0.9,
        "L_ecran_accuracy": 0.9,
        "combined_r2": 0.8
      },
      "tolerance": {
        "gap_tolerance": 0.01,
        "L_ecran_tolerance": 0.1
      },
      "metrics": [
        "r2_score",
        "mean_absolute_error",
        "mean_squared_error",
        "accuracy_within_tolerance"
      ]
    },
    "robustness": {
      "noise_testing": {
        "enable": true,
        "noise_levels": [
          0.0,
          0.01,
          0.02,
          0.05,
          0.1
        ],
        "apply_to": "training_only",
        "distribution": "gaussian"
      },
      "cross_validation": {
        "enable": false,
        "folds": 5
      }
    },
    "paths": {
      "dataset_path": "data_generation/dataset_2D",
      "augmented_data_cache": "data/augmented_dataset.npz",
      "models_dir": "models/",
      "plots_dir": "plots/",
      "results_dir": "results/",
      "logs_dir": "logs/"
    },
    "visualization": {
      "plots": [
        "training_curves",
        "scatter_predictions_gap",
        "scatter_predictions_L_ecran",
        "residuals_analysis",
        "parameter_correlation",
        "noise_robustness"
      ],
      "figure_size": [
        12,
        8
      ],
      "dpi": 300,
      "style": "seaborn"
    },
    "logging": {
      "level": "INFO",
      "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
      "file": "logs/training.log"
    },
    "dual_output": {
      "loss_weights": {
        "gap_weight": 1.0,
        "L_ecran_weight": 1.0,
        "adaptive_weighting": false
      },
      "separate_metrics": true,
      "individual_validation": true
    }
  }
}